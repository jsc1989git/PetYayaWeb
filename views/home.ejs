<div class="col-12 text-center">
  <div class="main-content">
    <h1>Welcome to the Dashboard</h1>
    <form action="/add-post" method="POST">
      <input type="text" id="postContent" name="post[content]" placeholder="Write a post...">
      <button type="submit">Add Post</button>
    </form>
  </div>
  <div class="container mt-3">
    <% posts.forEach(post => { %>
      <div class="row py-2 mb-2">
        <div class="col text-start"><%= post.content %></div>
        <div class="col text-end"><%= new Date(post.createdAt).toLocaleString() %></div>
        <div class="col-auto">
          <a href="/edit-post/<%= post._id %>" class="btn btn-primary btn-sm" >Edit</a>
        <form class="d-inline" action="/delete-post/<%= post._id %>?_method=DELETE" method="POST" onsubmit="return confirm('Are you sure you want to delete this post?')">
            <button type="submit" class="btn btn-sm btn-danger">Delete</button>
        </form>
        </div>
      </div>
    <%  }); %>
  </div>
</div>


